@model List<Taskm>


@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<H1>Get All Data</H1>

<a asp-action="Create" asp-controller="Task">Add Data</a>

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">User ID</th>
      <th scope="col">ID</th>
      <th scope="col">Title</th>
      <th scope="col">Completed</th>
      <th scope="col"></th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    @foreach(var taskm in Model){
        <tr>
                <th scope="row">@taskm.userId</th>
                <td>@taskm.id</td>
                <td>@taskm.title</td>
                <td>@taskm.completed</td>
                <td><a asp-controller="Task" asp-action="Details" asp-route-taskId="@taskm.id">View</a></td>
                <td><a asp-controller="Task" asp-action="Update" asp-route-taskId="@taskm.id">Edit</a></td>
                <td><a asp-controller="Task" asp-action="Delete" asp-route-taskId="@taskm.id">Delete</a></td>
        </tr>
    }
  </tbody>
</table>